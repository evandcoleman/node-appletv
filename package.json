{
  "name": "node-appletv",
  "version": "1.1.0",
  "description": "A Node.js library for communicating with an Apple TV",
  "homepage": "https://github.com/evandcoleman/node-appletv",
  "bugs": "https://github.com/evandcoleman/node-appletv/issues",
  "keywords": [
    "apple tv",
    "appletv",
    "mrp",
    "media remote"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsc && cp -R ./src/lib/protos ./dist/lib/protos",
    "test": "mocha -r ts-node/register ./src/test/**/*.spec.ts",
    "coverage": "nyc --reporter=lcov mocha -r ts-node/register ./src/test/**/*.spec.ts",
    "clean": "rimraf dist",
    "docs": "rimraf docs && typedoc --mode modules --excludePrivate --excludeExternals --out ./docs --exclude ./src/test/**/* ./src"
  },
  "bin": {
    "appletv": "bin/appletv"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evandcoleman/node-appletv.git"
  },
  "author": "Evan Coleman <e@edc.me> (https://edc.me)",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^4.1.0",
    "caporal": "^1.3.0",
    "curve25519-n2": "^1.1.3",
    "ed25519": "0.0.4",
    "fast-srp-hap": "^1.0.1",
    "inquirer": "^7.0.4",
    "mdns": "^2.5.1",
    "ora": "^2.1.0",
    "protobufjs": "^6.8.8",
    "snake-case": "^2.1.0",
    "sodium": "^3.0.2",
    "uuid": "^3.4.0",
    "varint": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/inquirer": "0.0.37",
    "@types/mdns": "0.0.32",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.5.0",
    "@types/ora": "^1.3.5",
    "@types/sinon": "^7.5.1",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "mitm": "^1.7.0",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5"
  }
}
